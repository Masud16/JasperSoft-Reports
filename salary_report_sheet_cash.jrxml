	<queryString>
		<![CDATA[SELECT
      P.EMP_ID,P.BANK_ACCOUNT_NO,
      P.EMP_CODE,
       P.EMP_NAME,
       CASE WHEN $P{P_DESIGNATION_VIEW} =1 THEN (P.DESIGNATION) ELSE '##.##' END DESIGNATION,
       P.ALKP_JOB_ID,
       P.Ou_TITLE,
       P.PRODUCT_TITLE,
             NVL(P.org_LOC_TITLE,(SELECT TITLE FROM ALKP WHERE ID=JOB_LOCATION_ID))LOCATION_TITLE,
      P.DEPARTMENT_TITLE,
      NVL(P.SECTION_TITLE,P.DEPARTMENT_TITLE)SECTION_TITLE,
      P.SUB_SECTION_TITLE,
       round(P.PRL_ELMNT_BASIC)PRL_ELMNT_BASIC,
       P.PRL_ELMNT_HOUSE_RENT,
       P.MEDICAL_ALLOWANCE,
       P.CONVEYANCE_ALLOWANCE,
       P.OTHER_ALLOWANCE,
       P.GROSS_rate_hr,
       P.PRL_ELMNT_LOAN,
       P.PRL_ELMNTAIT,
       P.PRL_ELMNTRPF,
P.PRL_ELMNT_INCENTIVE,
       P.PRL_ELMNT_CONVEYANCE_DEDUCTION,
       P.PRL_ELMNT_FOOD_DEDUCTION,
             (COALESCE(P.PRL_ELMNT_AWARD,0)-COALESCE(P.PRL_ELMNT_PUNISHMENT,0)) AWARD_PUNISHMENT,
       (COALESCE(P.PRL_ELMNT_OTHER_DEDUCTION,0)+ COALESCE(P.PRL_ELMNT_PENALTY_DEDUCTION,0)) PRL_ELMNT_OTHER_DEDUCTION,
       P.PRL_ELMNT_ABSENT_DEDUCTION,
       P.PRL_ELMNT_TEMP_ADV_DEDUCTION,
       P.TOTAL_DEDUCTION,
       round(P.NET_PAYABLE_AMOUNT) NET_PAYABLE_AMOUNT,
       P.ORG_LOC_TITLE,
             P.location
        FROM PRL_SAL_TBL P
   WHERE 1 = 1 
       AND ( P.IS_SALARY_HOLD= $P{P_IS_SAL_HOLD} )
        AND ($P{P_HR_PRL_MST_ID} is null or P.HR_PRL_MST_ID=$P{P_HR_PRL_MST_ID} )
AND  ($P{P_ALKP_EMP_CAT_ID} is null or P.ALKP_EMP_CAT_ID = $P{P_ALKP_EMP_CAT_ID} )
AND ( $P{P_IS_BANK_SAL} is null or ( $P{P_IS_BANK_SAL} = 1 AND P.BANK_ACCOUNT_NO IS NOT NULL) OR  ($P{P_IS_BANK_SAL}  = 0 AND P.BANK_ACCOUNT_NO IS NULL) )
        AND P.FROM_DATE =TO_DATE ($P{P_START_DATE},'yyyy-MM-dd')
        AND  P.TO_DATE = TO_DATE ($P{P_END_DATE} ,'yyyy-MM-dd')
       AND  $X{IN,P.emp_code, P_EMP_CODES}
                             
AND ($P{P_JOB_LOCATION_ID} is null or P.JOB_LOCATION_ID= $P{P_JOB_LOCATION_ID})
        ORDER BY    6,9,10,11,(CASE when p.emp_code LIKE '%A-%' THEN (REPLACE(p.emp_code,'A-',''))
 when p.emp_code LIKE '%D-%' THEN (REPLACE(p.emp_code,'D-',''))
when p.emp_code LIKE '%C-%' THEN (REPLACE(p.emp_code,'C-',''))
 ELSE
  (p.emp_code)
 END)]]>
	</queryString>
