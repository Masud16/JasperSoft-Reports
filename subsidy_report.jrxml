	<queryString>
		<![CDATA[SELECT P.EMP_CODE,
         P.EMP_NAME,
         CASE WHEN $P{P_DESIGNATION_VIEW} ='1' THEN (P.DESIGNATION) ELSE '##.##' END DESIGNATION,
         P.Ou_TITLE,
         P.PRODUCT_TITLE,
         P.DEPARTMENT_TITLE,
         NVL(P.SECTION_TITLE,P.DEPARTMENT_TITLE) SECTION_TITLE,
         P.SUB_SECTION_TITLE,
         (SELECT TITLE
            FROM ALKP
           WHERE ID = P.JOB_LOCATION_ID)
            JOB_LOCATION,
         COALESCE (P.PRL_ELMNT_FOOD_DEDUCTION, 0) FOOD_DEDUCTION,
         COALESCE (P.PRL_ELMNT_SUBSIDY, 0) SUBSIDY_DEDUCTION,
                COALESCE((SELECT SUM(M.SUBSIDISE)
  FROM HR_CR_FOOD_MESS M, HR_CR_FOOD_EMP_LK L
 WHERE M.ID = L.HR_CR_FOOD_MESS_ID_hr_cr_food_mess AND L.HR_CR_EMP_ID_hr_cr_emp = P.EMP_ID
AND ( TO_DATE($P{P_START_DATE} ,'YYYY-MM-DD') BETWEEN L.START_DATE  AND COALESCE ( L.END_DATE,  TO_DATE($P{P_END_DATE} ,'YYYY-MM-DD'))  
 AND  TO_DATE($P{P_START_DATE} ,'YYYY-MM-DD') BETWEEN  L.START_DATE AND COALESCE (L.END_DATE,  TO_DATE($P{P_END_DATE} ,'YYYY-MM-DD') ) )
 ),0) FOOD_SUBSIDIES,
  ((COALESCE((SELECT SUM(M.SUBSIDISE)
  FROM HR_CR_FOOD_MESS M, HR_CR_FOOD_EMP_LK L
 WHERE M.ID = L.HR_CR_FOOD_MESS_ID_hr_cr_food_mess AND L.HR_CR_EMP_ID_hr_cr_emp = P.EMP_ID
AND ( TO_DATE($P{P_START_DATE} ,'YYYY-MM-DD') BETWEEN L.START_DATE  AND COALESCE ( L.END_DATE,  TO_DATE($P{P_END_DATE} ,'YYYY-MM-DD'))  
 AND  TO_DATE($P{P_START_DATE} ,'YYYY-MM-DD') BETWEEN  L.START_DATE AND COALESCE (L.END_DATE,  TO_DATE($P{P_END_DATE} ,'YYYY-MM-DD') ) )
 ),0))-(COALESCE (P.PRL_ELMNT_SUBSIDY, 0))) d_balance
    FROM PRL_SAL_TBL P
   WHERE     1 = 1
    AND ($P{P_HR_PRL_MST_ID} is null or P.HR_PRL_MST_ID = $P{P_HR_PRL_MST_ID})
         AND P.FROM_DATE =TO_DATE($P{P_START_DATE} ,'YYYY-MM-DD') 
         AND P.TO_DATE = TO_DATE($P{P_END_DATE} ,'YYYY-MM-DD') 
       AND  ($P{P_ALKP_EMP_CAT_ID} is null or P.ALKP_EMP_CAT_ID=$P{P_ALKP_EMP_CAT_ID})
 AND  $X{IN,P.emp_code, P_EMP_CODES}
AND  ($P{P_JOB_LOCATION_ID} is null or P.JOB_LOCATION_ID=$P{P_JOB_LOCATION_ID}) 
AND COALESCE (PRL_ELMNT_FOOD_DEDUCTION, 0)>0
ORDER BY 4,6,7,8,
         (CASE
             WHEN P.EMP_CODE LIKE '%A-%'
             THEN
             (REPLACE (P.EMP_CODE, 'A-', ''))
             WHEN P.EMP_CODE LIKE '%D-%'
             THEN
                 (REPLACE (P.EMP_CODE, 'D-', ''))
             WHEN P.EMP_CODE LIKE '%C-%'
             THEN
                 (REPLACE (P.EMP_CODE, 'C-', ''))
             ELSE
                 (P.EMP_CODE)
          END)]]>
	</queryString>
