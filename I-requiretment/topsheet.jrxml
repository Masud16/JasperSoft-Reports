	<queryString>
		<![CDATA[SELECT 
  a.CODE,
  b.id hr_ir_vcncy_id,
  a.FATHER_NAME, 
  a.national_identity_number, 
  AGE(a.DOB)::varchar Age, 
  a.MOTHER_NAME, 
  a.joining_date,
  a.viva_mark_1,
a.viva_mark_2,
a.viva_mark_3,
a.viva_mark_4,
a.viva_mark_5,
a.viva_mark_6,
a.viva_mark_7,
a.viva_mark_8,
(select display_name || ' ( ' || code || ' ) ' as viva_1_pnl_mbr_name_id from hr_cr_emp where id = a.viva_1_hr_cr_emp_id ), 
(select display_name || ' ( ' || code || ' ) ' as viva_2_pnl_mbr_name_id from hr_cr_emp where id = a.viva_2_hr_cr_emp_id ),
(select display_name || ' ( ' || code || ' ) ' as viva_3_pnl_mbr_name_id from hr_cr_emp where id = a.viva_3_hr_cr_emp_id ),
(select display_name || ' ( ' || code || ' ) ' as viva_4_pnl_mbr_name_id from hr_cr_emp where id = a.viva_4_hr_cr_emp_id ),
(select display_name || ' ( ' || code || ' ) ' as viva_5_pnl_mbr_name_id from hr_cr_emp where id = a.viva_5_hr_cr_emp_id ),
(select display_name || ' ( ' || code || ' ) ' as viva_6_pnl_mbr_name_id from hr_cr_emp where id = a.viva_6_hr_cr_emp_id ),
(select display_name || ' ( ' || code || ' ) ' as viva_7_pnl_mbr_name_id from hr_cr_emp where id = a.viva_7_hr_cr_emp_id ),
(select display_name || ' ( ' || code || ' ) ' as viva_8_pnl_mbr_name_id from hr_cr_emp where id = a.viva_8_hr_cr_emp_id ),
  (
    SELECT 
      TITLE 
    FROM 
      ALKP 
    WHERE 
      ID = a.alkp_job_type_id
  ) JOB_CATEGORY,
  (
    SELECT 
      TITLE 
    FROM 
      ALL_LOC
    WHERE 
      ID = a.ALL_LOC_ADDRSS_PRMNT_ID
  ) PS_PRMNT, 
  (
    SELECT 
      FULL_NAME 
    FROM 
      ALL_LOC 
    WHERE 
      ID = a.ALL_LOC_ADDRSS_PRMNT_ID
  ) DIST_PRMNT, 
  (
    SELECT 
      title 
    FROM 
      ALL_ORG_MST 
    WHERE 
      ID = a.all_org_operating_unit_id :: bigint
  ) APP_LOCATION, 
  (
    SELECT 
      FULL_NAME 
    FROM 
      ALL_LOC 
    WHERE 
      ID = a.ALL_LOC_ADDRSS_PRSNT_ID
  ) PRESENT_THANA, 
  a.designation PRO_DESIGNATION, 
  (a.DOB) DOB, 
  a.TITLE NAME, 
  a.ADDRSS_PRMNT_PO, 
  a.ADDRSS_PRSNT_BNG, 
  a.ADDRSS_PRSNT_PC, 
  (
    CASE WHEN a.is_rejected = false THEN 'Acceptable' ELSE 'Not Acceptable' END
  ) AS ISPASSED, 
  a.ADDRSS_PRSNT_PO, 
  a.permanent_address, 
  (
    Floor(a.HEIGHT / 12)|| '''' || MOD(a.HEIGHT, 12)|| ''''''
  ) HEIGHT, 
  a.WEIGHT, 
  a.CHEST_SIZE, 
  a.MOB_CODE, 
  a.it_marks, 
  --c.MARKS_OUT_OF_IT, 
  a.written_marks, 
  a.OVER_PHONE, 
  --c.MARKS_OUT_OF_WRT, 
  a.is_approved IS_SUITABLE, 
  a.PIC_URL, 
  (
    SELECT 
      TITLE 
    FROM 
      ALKP 
    WHERE 
      ID = a.ALKP_MARITAL_STS_ID
  ) ISMARIED, 
  a.sal_expected EXPECTED_SAL, 
  a.salary APRVD_SAL, 
  a.sal_curr PRESENT_SAL, 
  a.sal_nego PROPOSED_SAL, 
  a.prime_duty APPONITED_FOR, 
(
        COALESCE(a.viva_mark_1, '0')::float + COALESCE(a.viva_mark_2, '0')::float + COALESCE(a.viva_mark_3, '0')::float + COALESCE(a.viva_mark_4, '0')::float + COALESCE(a.viva_mark_5, '0')::float + COALESCE(a.viva_mark_6, '0')::float + COALESCE(a.viva_mark_7, '0')::float + COALESCE(a.viva_mark_8, '0')::float
      ) AS Total_Viva_Marks, 
  (
    SELECT 
      TITLE 
    FROM 
      ALKP 
    WHERE 
      ID = ALKP_BLOOD_GRP_ID
  ) Blood_Group ,
   pre_job_experience(a.id::bigint, 1) as Last_Experience, 
   pre_job_experience(a.id::bigint, 2) as REFERENCE, 
  (
    Select 
      TITLE 
    from 
      ALKP 
    Where 
      id =alkp_job_location_id
  ) LOCATION_NAME, 
  a.designation Designation_tittle,
  (
    Select 
      salary 
    from 
      HR_IR_APLC 
    where 
      ID = a.id 
      AND cast(HR_IR_VCNCY_ID_2 as integer) = b.ID
  ) APPROVED_OSAM,
  a.probation_duration DURATION,
  a.hrm_comments HR_COMENTS,
  (
    Select 
      TITLE 
    from 
      ALL_ORG_MST 
    Where 
      id =(
        Select 
           all_org_mst_operating_unit_id
        from 
          HR_IR_APLC 
        where 
           id = a.id 
          AND cast(HR_IR_VCNCY_ID_2 as integer) = b.ID
      )
  ) ALL_CP,
  
  (SELECT 
  (
    new_tbl.sum / new_tbl.total_no_of_viva
  ) 
FROM 
  (
    SELECT x.id,
      (
        COALESCE(x.viva_mark_1, '0')::float + COALESCE(x.viva_mark_2, '0')::float + COALESCE(x.viva_mark_3, '0')::float + COALESCE(x.viva_mark_4, '0')::float + COALESCE(x.viva_mark_5, '0')::float + COALESCE(x.viva_mark_6, '0')::float + COALESCE(x.viva_mark_7, '0')::float + COALESCE(x.viva_mark_8, '0')::float
      ) AS sum, 
      CASE WHEN (
        (x.viva_mark_1 IS NOT NULL)::integer + (x.viva_mark_2 IS NOT NULL)::integer + (x.viva_mark_3 IS NOT NULL)::integer + (x.viva_mark_4 IS NOT NULL)::integer + (x.viva_mark_5 IS NOT NULL)::integer + (x.viva_mark_6 IS NOT NULL)::integer + (x.viva_mark_7 IS NOT NULL)::integer + (x.viva_mark_1 IS NOT NULL)::integer
      ) = 0 THEN 1 ELSE (
        (x.viva_mark_1 IS NOT NULL)::integer + (x.viva_mark_2 IS NOT NULL)::integer + (x.viva_mark_3 IS NOT NULL)::integer + (x.viva_mark_4 IS NOT NULL)::integer + (x.viva_mark_5 IS NOT NULL)::integer + (x.viva_mark_6 IS NOT NULL)::integer + (x.viva_mark_7 IS NOT NULL)::integer + (x.viva_mark_1 IS NOT NULL)::integer
      ) END AS total_no_of_viva 
    FROM 
      hr_ir_aplc x 
  ) AS new_tbl 
  where new_tbl.id=a.id) as AVG_VIVA_MARK
  
FROM 
  HR_IR_APLC a ,
  hr_ir_vcncy b 
WHERE 
   a.CODE = $P{P_CODE} 
   AND b.ID = $P{P_VCNCY_ID}
limit 1]]>
