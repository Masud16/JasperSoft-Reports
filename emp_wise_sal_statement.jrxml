	<queryString>
		<![CDATA[SELECT
      A.ID,
      A.EMP_CODE,
       A.EMP_NAME,
       CASE WHEN $P{P_DESIGNATION_VIEW} =1 THEN (P.DESIGNATION) ELSE  '##.##' END DESIGNATION,
       A.ALKP_JOB_ID,
       A.OPERATING_UNIT_TITLE,
        A.LOCATION_TITLE,
       A.DEPARTMENT_TITLE,
       P. SECTION_TITLE,
INITCAP(TO_CHAR(P.FROM_DATE,'MONTH-YYYY'))FROM_DATE,
P.TO_DATE,
       P.PRL_ELMNT_BASIC,
       P.PRL_ELMNT_HOUSE_RENT,
       P.MEDICAL_ALLOWANCE,
       P.CONVEYANCE_allowance,
       P.OTHER_allowance,
      P.PRL_ELMNT_INCENTIVE PRL_ELMNT_INCENTIVE,
       P.GROSS_RATE_HR,
       P.PRL_ELMNT_LOAN,
       P.PRL_ELMNTAIT,
      P.PRL_ELMNTRPF,
       P.PRL_ELMNT_CONVEYANCE_DEDUCTION,
       P.PRL_ELMNT_FOOD_DEDUCTION,
       ((P.PRL_ELMNT_OTHER_DEDUCTION)+ (P.PRL_ELMNT_PENALTY_DEDUCTION))PRL_ELMNT_OTHER_DEDUCTION,
       P.PRL_ELMNT_ABSENT_DEDUCTION,
       P.PRL_ELMNT_TEMP_ADV_DEDUCTION,
       P.TOTAL_DEDUCTION,
       round(P.NET_PAYABLE_AMOUNT),
       P.ORG_LOC_TITLE,
       P.OU_ADDRESS
FROM HR_CR_EMP_PRIMARY_ASSGNMNT A,PRL_SAL_TBL P
WHERE 1=1
AND A.ID=P.EMP_ID
AND (P.FROM_DATE BETWEEN   TO_DATE (  $P{P_START_DATE} , 'yyyy-MM-DD')
                                     AND TO_DATE ($P{P_END_DATE} ,  'yyyy-MM-DD'))
AND ( P.TO_DATE BETWEEN  TO_DATE (  $P{P_START_DATE} , 'yyyy-MM-DD')
                                     AND TO_DATE ($P{P_END_DATE} ,  'yyyy-MM-DD'))
 AND $X{IN,P.emp_code,P_EMP_CODES}
ORDER BY P.FROM_DATE]]>
	</queryString>
