	<queryString language="SQL">
		<![CDATA[SELECT 
  e.display_name supervisor_name, 
  e.login_code supervisor_id, 
  d.display_name investigator_name, 
  d.login_code investigator_id, 
  c.title category_name, 
  a.*, 
  b.emp_name, 
  b.emp_code, 
  b.department_title, 
  b.operating_unit_title, 
  b.section_title, 
  b.sub_section_title 
FROM 
  audit_investigation_proc a 
  inner join hr_cr_emp_primary_assgnmnt b ON a.hr_cr_emp_id_id = b.hr_cr_emp_id 
  left join alkp c ON a.alkp_cat_id = c.id 
  left join hr_cr_emp d ON a.investigator_id = d.id 
  left join hr_cr_emp e ON a.supervisor_id = e.id 
WHERE 
  b.emp_code = $P{APPLICANT_EMP_CODE}
]]>
	</queryString>
